#include <stdio.h>

int main() {
    int n,m ;
    printf("enter the n:");
    scanf("%d",&n);
    printf("enter the m:");
    scanf("%d",&m);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            if(i==1 || i==6 || j==1 || j==14){
                printf("*");}
                else{
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
